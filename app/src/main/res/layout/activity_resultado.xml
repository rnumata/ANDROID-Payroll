<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultadoActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Payroll"
        android:textAlignment="center"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtNome"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:text="Funcionario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/txtHoras"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="Horas Trabalhadas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNome" />

    <TextView
        android:id="@+id/txtValorHora"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="Valor da Hora"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtHoras" />

    <TextView
        android:id="@+id/txtSalBruto"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="Salario Bruto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtValorHora" />

    <TextView
        android:id="@+id/txtIr"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="IR"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtSalBruto" />

    <TextView
        android:id="@+id/txtInss"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="INSS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtIr" />

    <TextView
        android:id="@+id/txtFgts"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="FGTS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtInss" />

    <TextView
        android:id="@+id/txtSalLiq"
        android:layout_width="285dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:text="Salario Liquido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFgts" />

</androidx.constraintlayout.widget.ConstraintLayout>